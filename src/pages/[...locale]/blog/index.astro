---
const { locale } = Astro.params;

import Layout from "../../../layouts/Layout.astro";

export const getStaticPaths = async () => {
  return [{ params: { locale: "en" } }, { params: { locale: undefined } }];
};
---
<Layout title="Blog: mi diario creativo" description="Espacio abierto para compartir y aprender sobre animación, diseño, ilustración  y narrativa" active="blog">
  Blog index
  {locale}
</Layout>
