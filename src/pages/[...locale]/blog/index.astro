---
const { locale } = Astro.params;

import Layout from "../../../layouts/Layout.astro";

export const getStaticPaths = async () => {
  return [{ params: { locale: "en" } }, { params: { locale: undefined } }];
};
---
<Layout title="Blog: mi día a día creativo" description="Espacio abierto para compartir y dialogar sobre arte, animación y diseño" active="blog">
  Blog index
  {locale}
</Layout>
