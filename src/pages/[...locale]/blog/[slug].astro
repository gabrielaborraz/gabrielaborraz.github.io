---
const { locale, slug } = Astro.params;

import Layout from "../../../layouts/Layout.astro";

export const getStaticPaths = async () => {
  return [
    { params: { locale: "en", slug: "post1-en" } },
    { params: { locale: undefined, slug: "post1-es" } },
  ];
};
---
<Layout title={slug || ""} description="Aquí breve descripción del post" active="blog">
  {slug}
  {locale}
</Layout>
